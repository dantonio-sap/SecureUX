{
	"welcomeFile": "webapp/index.html",
	"authenticationMethod": "route",
	"routes": [
		{
			"source": "^/(.*)(.xsjs)",
			"destination": "core_api",
			"authenticationType": "xsuaa"
		},
		{
			"source": "^/(.*)(.xsodata)",
			"destination": "core_api",
			"authenticationType": "xsuaa"
		},
		{
			"source": "^/(.*)$",
    			"localDir" : "resources"
		}
	]
}
