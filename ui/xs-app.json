{
	"welcomeFile": "webapp/index.html",
	"authenticationMethod": "route",
	"routes": [
		{
			"source": "^/xsodata/(.*)",
			"destination": "core_api",
			"authenticationType": "xsuaa",
			"scope" : "$XSAPPNAME.Display"
		},
		{
			"source": "^/(.*)$",
    		"localDir" : "resources"
		}
	]
}
